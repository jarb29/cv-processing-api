{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "CVProcessing": "Debug"
    }
  },
  "OpenAI": {
    "ApiKey": "your-development-openai-api-key",
    "Model": "gpt-4o-mini",
    "MaxTokens": 2000,
    "Temperature": 0.2,
    "TimeoutSeconds": 60
  },
  "Storage": {
    "Type": "Local",
    "Path": "./storage-dev"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "CVProcessing": "Debug"
      }
    }
  }
}